cell_code["$divfloor+"] = "module \\$divfloor (A, B, Y);\n"
"    \n"
"    parameter A_SIGNED = 0;\n"
"    parameter B_SIGNED = 0;\n"
"    parameter A_WIDTH = 0;\n"
"    parameter B_WIDTH = 0;\n"
"    parameter Y_WIDTH = 0;\n"
